{
  "name": "ifcastle/aql-migration-tool",
  "description": "Migration tool for AQL",
  "keywords": [
    "aql",
    "query",
    "language",
    "migration"
  ],
  "type": "ifcastle-aql-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Edmond Dantes",
      "email": "edmondifthen@proton.me"
    }
  ],
  "require": {
    "php": ">=8.5"
  },
  "autoload": {
    "psr-4": {
      "IfCastle\\AQL\\MigrationTool\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IfCastle\\AQL\\MigrationTool\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "rector/rector": "^1.2",
    "ifcastle/codestyle": "^0.2.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit ./tests"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.8.x-dev"
    }
  }
}
